generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("POSTGRES_URL")
}

model Room {
    id               String   @id @default(uuid())
    createdAt        DateTime @default(now())
    updatedAt        DateTime @updatedAt
    roomCreator      String?
    roomCreatorName  String
    roomOccupant     String?
    roomOccupantName String?
    roomToken        String   @unique
}
